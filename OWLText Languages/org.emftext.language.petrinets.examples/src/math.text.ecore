package math // this is the package name 
        math // this is the namespace prefix
        "http://org.emftext/languages/math.ecore" // the namespace URI 
	{

	class Expression {
		containment reference Operator operator (1..1);
		containment reference Elements elements (1..-1);
		attribute EString name (1..1);
	}
	
	class Elements {
		attribute EString id (1..1);
	}
	
	enum Operator {
		1 : PLUS = "add";
		2 : MINUS = "subtract";
		3 : STAR = "multiply";
	}
}