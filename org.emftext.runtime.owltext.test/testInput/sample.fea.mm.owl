Prefix: xsd: <http://www.w3.org/2001/XMLSchema#> 
	Prefix: feature: <http://org.owltext/feature#> 
	Prefix: owl: <http://www.w3.org/2002/07/owl#> 
	Prefix: : <http://org.owltext/feature#> 
	Prefix: rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
	Prefix: rdfs: <http://www.w3.org/2000/01/rdf-schema#> 
	Ontology: <http://org.owltext/feature> 
	
	Class: Feature 
		SubClassOf:  (  Feature_name min 1  xsd:string ) and  (  Feature_name max 1  xsd:string ) and  (  Feature_parent max 1  Feature ) 
			
		
	Class: MandatoryFeature 
		SubClassOf:  Feature 
			
		
	Class: OptionalFeature 
		SubClassOf:  Feature 
			
		
	DataProperty: Feature_name 
		Domain:  Feature 
			Range:  xsd:string 
			Characteristics: Functional 
			
		
	DataProperty: Feature_annotations 
		Domain:  Feature 
			Range:  xsd:string 
			Characteristics: Functional 
			
		
	ObjectProperty: Feature_children 
		Domain:  Feature 
			Range:  Feature 
			InverseOf: Feature_parent , Feature_parent 
			
		
	ObjectProperty: Feature_parent 
		Domain:  Feature 
			Range:  Feature 
			Characteristics: Functional 
			InverseOf: Feature_children , Feature_children 
			
		